{
    "version": "1.0",
    "project": "LangGraph Multi-Agent AI",
    "description": "Configuration for deploying the LangGraph multi-agent AI project via LangGraph Studio.",
    "entry_point": "./langstuff_multi_agent/agent.py:graph",
    "graphs": {
        "main": "./langstuff_multi_agent/agent.py:graph",
        "debugger_graph": "./langstuff_multi_agent/agent.py:debugger_graph",
        "context_manager_graph": "./langstuff_multi_agent/agent.py:context_manager_graph",
        "project_manager_graph": "./langstuff_multi_agent/agent.py:project_manager_graph",
        "professional_coach_graph": "./langstuff_multi_agent/agent.py:professional_coach_graph",
        "life_coach_graph": "./langstuff_multi_agent/agent.py:life_coach_graph",
        "coder_graph": "./langstuff_multi_agent/agent.py:coder_graph",
        "analyst_graph": "./langstuff_multi_agent/agent.py:analyst_graph",
        "researcher_graph": "./langstuff_multi_agent/agent.py:researcher_graph",
        "general_assistant_graph": "./langstuff_multi_agent/agent.py:general_assistant_graph",
        "news_reporter_graph": "./langstuff_multi_agent/agent.py:news_reporter_graph",
        "customer_support_graph": "./langstuff_multi_agent/agent.py:customer_support_graph",
        "marketing_strategist_graph": "./langstuff_multi_agent/agent.py:marketing_strategist_graph",
        "creative_content_graph": "./langstuff_multi_agent/agent.py:creative_content_graph",
        "financial_analyst_graph": "./langstuff_multi_agent/agent.py:financial_analyst_graph"
    },
    "agents": [
        {
            "name": "SUPERVISOR",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "main",
            "description": "Main supervisor agent that routes requests to specialized agents."
        },
        {
            "name": "DEBUGGER",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "debugger_graph",
            "description": "Agent responsible for debugging code."
        },
        {
            "name": "CONTEXT_MANAGER",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "context_manager_graph",
            "description": "Agent responsible for managing conversation context."
        },
        {
            "name": "PROJECT_MANAGER",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "project_manager_graph",
            "description": "Agent responsible for managing project timelines and tasks."
        },
        {
            "name": "PROFESSIONAL_COACH",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "professional_coach_graph",
            "description": "Agent providing professional and career guidance."
        },
        {
            "name": "LIFE_COACH",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "life_coach_graph",
            "description": "Agent providing lifestyle and personal advice."
        },
        {
            "name": "CODER",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "coder_graph",
            "description": "Agent that assists with coding tasks."
        },
        {
            "name": "ANALYST",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "analyst_graph",
            "description": "Agent specializing in data analysis."
        },
        {
            "name": "RESEARCHER",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "researcher_graph",
            "description": "Agent that gathers and summarizes research and news."
        },
        {
            "name": "GENERAL_ASSISTANT",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "general_assistant_graph",
            "description": "Agent for general queries and assistance."
        },
        {
            "name": "NEWS_REPORTER",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "news_reporter_graph",
            "description": "Agent that searches, researches, explains and summarizes news reports."
        },
        {
            "name": "CUSTOMER_SUPPORT",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "customer_support_graph",
            "description": "Agent that provides customer support and assistance."
        },
        {
            "name": "MARKETING_STRATEGIST",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "marketing_strategist_graph",
            "description": "Agent that provides marketing strategy, insights, trends, and planning."
        },
        {
            "name": "CREATIVE_CONTENT",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "creative_content_graph",
            "description": "Agent that provides creative content, marketing copy, social media posts, or brainstorming ideas."
        },
        {
            "name": "FINANCIAL_ANALYST",
            "file": "./langstuff_multi_agent/agent.py",
            "graph": "financial_analyst_graph",
            "description": "Agent that provides financial analysis, market data, forecasting, and investment insights."
        }
    ],
    "dependencies": [
        "langgraph>=0.0.20",
        "langchain-anthropic>=0.0.10",
        "langchain-core>=0.1.20",
        "langchain-openai>=0.0.5",
        "python-dotenv>=1.0.0",
        "tavily-python>=0.5.1",
        "langchain_community>=0.3.17",
        "./langstuff_multi_agent"
    ],
    "configuration": {
        "xai_api_key": "${XAI_API_KEY}",
        "anthropic_api_key": "${ANTHROPIC_API_KEY}",
        "openai_api_key": "${OPENAI_API_KEY}",
        "tavily_api_key": "${TAVILY_API_KEY}",
        "model_settings": {
            "default_provider": "openai",
            "default_model": "gpt-4o-mini",
            "default_temperature": 0.4,
            "default_top_p": 0.9,
            "default_max_tokens": 4000
        },
        "checkpointer": "MemorySaver",
        "logging": {
            "level": "INFO",
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    }
}